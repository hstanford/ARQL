{
  "name": "test",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/test/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/test",
        "main": "apps/test/src/index.ts",
        "tsConfig": "apps/test/tsconfig.lib.json",
        "assets": ["apps/test/*.md"]
      }
    },
    "run": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "./apps/test",
        "command": "TS_NODE_PROJECT=./tsconfig.lib.json NX_BASE_TSCONFIG=../../tsconfig.base.json node --loader nx-ts-esm-loader -r tsconfig-paths/register src/index.ts"
      }
    },
    "db": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "./apps/test",
        "command": "docker compose run db"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/test/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/test/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
